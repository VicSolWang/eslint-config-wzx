{
	"env": {
		"browser": true
	},
	"extends": "eslint-config-airbnb-base",
	"rules": {
		"function-paren-newline": "off",
		"global-require": "off",
		"indent": [
			"error",
			"tab"
		],
		"implicit-arrow-linebreak": "off",
		"import/no-dynamic-require": "off",
		"no-console": [
			"error",
			{
				"allow": [
					"warn",
					"error"
				]
			}
		],
		"no-nested-ternary": "off",
		"no-tabs": "off",
		"object-curly-newline": [
			"error",
			{
				"consistent": true
			}
		]
	}
}
